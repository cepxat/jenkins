        stage("SCM Clone"){
            steps{
                sh "date"
                sh "echo 'welcome to steps in stage checkout'"
            }
        }
                stage("Build"){
            steps{
                sh "date"
                sh "echo 'welcome to steps in stage checkout'"
            }
        }
                stage("Test"){
            steps{
                sh "date"
                sh "echo 'welcome to steps in stage checkout'"
            }
        }
    }
}
